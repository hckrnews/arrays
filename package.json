        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-prettier": "^4.0.0",
        "jscpd": "^3.4.5",
        "microbundle": "^0.15.1",
        "prettier": "^2.5.0"
    },
    "type": "module",
    "engines": {
        "node": ">= 18"
    },
    "funding": {
