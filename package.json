{
    "name": "@hckrnews/arrays",
    "version": "3.1.2",
    "description": "Usefull array helpers.",
    "files": [
        "src/helpers.js",
        "src/modules/average.js",
        "src/modules/diff.js",
        "src/modules/first.js",
        "src/modules/getByKey.js",
        "src/modules/intersect.js",
